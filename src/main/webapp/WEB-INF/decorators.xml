<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>*/css/*</pattern>
		<pattern>*/images/*</pattern>
		<pattern>*/ajax/*</pattern>
		<pattern>*/up/*</pattern>
		<pattern>/login.html</pattern>
		<pattern>/register.html</pattern>
		<pattern>/aip/*</pattern>
		<pattern>/frvedio.html</pattern>
		<pattern>*/open/*</pattern>
		<pattern>*.json</pattern>
		<pattern>*/wap/*</pattern>
		
		<!-- <pattern>/gofindpasswd.html</pattern> -->
	</excludes>
	

	
	<decorator name="none">  
      <pattern>*.html</pattern>
    </decorator>
    
</decorators>
